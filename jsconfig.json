{
  "compilerOptions": {
    "paths": {
      "@components": ["@vendor/lib/components/index.js"],
      "@components/*": ["@vendor/lib/components/*"],

      "@services/*": ["./@vendor/lib/services/*", "./@services/*"],

      "@services": ["./@vendor/lib/services/index.js"],
      "@styles": ["./@vendor/lib/styles/styles.scss"],
      "@sass-variables": ["./@vendor/lib/styles/variables.output.scss"],

      "@modules/*": ["./@vendor/lib/modules/*"],
      //build
      "@props": ["./@vendor/lib/props/index.js"],
      "@props/*": ["./@vendor/lib/props/*"],
      "@vendor/*": ["./@vendor/*"],
      "@database/*": ["./@database/*"],
      "@database": ["./@vendor/lib/database/index.js"],
      "@dashboard": ["./@application/dashboard/index.js"],
      "@application/*": ["./@application/*"]
    }
  },
  "include": ["src/**/*", "lib/config", "./", "dev.js"]
}
