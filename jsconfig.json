{
  "compilerOptions": {
    "paths": {
      "@*": ["./src/*", "./*"],
      "@components": ["./vendor/components/index.js"],
      "@components/*": ["./vendor/components/*"],
      "@services/*": ["./vendor/services/*"],
      "@services": ["./vendor/services/index.js"],
      "@styles": ["./vendor/styles/styles.scss"],
      "@sass-variables": ["./vendor/styles/variables.output.scss"],
      "@modules/*": ["./vendor/modules/*"],
      "@candy/*": ["./candy/*"]
    }
  },
  "include": ["./src/**/*", "lib/config", "vendor", "candy"]
}
