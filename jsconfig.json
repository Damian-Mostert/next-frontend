{
  "compilerOptions": {
    "paths": {
      "@components": ["./src/app/lib/components/index.js"],
      "@components/*": ["./src/app/lib/components/*"],

      "@services/*": ["./src/app/lib/services/*", "./@services/*"],

      "@services": ["./src/app/lib/services/index.js"],
      "@styles": ["./src/build/styles/styles.scss"],
      "@sass-variables": ["./src/build/styles/variables.output.scss"],
      "@modules/*": ["./src/lib/modules/*"],
      "@database/*": ["./src/lib/database/*"]
    }
  },
  "include": ["src/**/*", "lib/config", "./", "dev.js"]
}
