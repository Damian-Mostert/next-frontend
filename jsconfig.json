{
  "compilerOptions": {
    "paths": {
      "@/*": ["././src/*"],
      "@config/*": ["././src/app/config/*"],
      "@components": ["./src/utils/components/index.js"],
      "@components/*": ["./src/utils/components/*"],
      "@services/*": [
        "./src/utils/services/*",
        "./src/utils/services/index.js"
      ],
      "@styles": ["./src/utils/styles/styles.scss"],
      "@sass-variables": ["./src/utils/styles/variables.output.scss"],
      "@static": ["./src/lib/components/static/index.js"],
      "@forms/*": ["./src/lib/forms/*"],
      "@modules/*": ["./src/lib/modules/*", "./src/utils/modules/*"],
      "@navigation/*": ["./src/lib/navigation/*"]
    }
  },
  "include": ["./src/**/*"]
}
